ener(\"click\", increment);\r\n  });\r\n\r\nexport default {\r\n  props: {\r\n    articleCount: {\r\n      type: Number,\r\n      default: 0,\r\n    },\r\n  },\r\n  methods: {\r\n    updateArticleCount(event) {\r\n      this.$emit('updateArticleCount', Number(event.target.value));\r\n    },\r\n  },\r\n};\r\n\r\n</script>\r\n"],"mappings":"AAiCC,SAASA,SAASA,CAACC,CAAC,EAAE;EACnB,MAAMC,GAAE,GAAID,CAAC,CAACE,MAAM,CAACC,UAAU,CAACC,aAAa,CAACC,aAAa,CACzD,iCACF,CAAC;EACD,MAAMH,MAAK,GAAID,GAAG,CAACK,kBAAkB;EACrC,IAAIC,KAAI,GAAIC,MAAM,CAACN,MAAM,CAACK,KAAK,CAAC;EAChCA,KAAK,EAAE;EACPL,MAAM,CAACK,KAAI,GAAIA,KAAK;AACtB;AAEA,SAASE,SAASA,CAACT,CAAC,EAAE;EACpB,MAAMC,GAAE,GAAID,CAAC,CAACE,MAAM,CAACC,UAAU,CAACC,aAAa,CAACC,aAAa,CACzD,iCACF,CAAC;EACD,MAAMH,MAAK,GAAID,GAAG,CAACK,kBAAkB;EACrC,IAAIC,KAAI,GAAIC,MAAM,CAACN,MAAM,CAACK,KAAK,CAAC;EAChCA,KAAK,EAAE;EACPL,MAAM,CAACK,KAAI,GAAIA,KAAK;AACtB;AAEA,MAAMG,gBAAe,GAAIC,QAAQ,CAACC,gBAAgB,CAC/C,iCACH,CAAC;AAED,MAAMC,gBAAe,GAAIF,QAAQ,CAACC,gBAAgB,CAC/C,iCACH,CAAC;AAEDF,gBAAgB,CAACI,OAAO,CAACb,GAAE,IAAK;EAC9BA,GAAG,CAACc,gBAAgB,CAAC,OAAO,EAAEhB,SAAS,CAAC;AAC1C,CAAC,CAAC;AAEFc,gBAAgB,CAACC,OAAO,CAACb,GAAE,IAAK;EAC9BA,GAAG,CAACc,gBAAgB,CAAC,OAAO,EAAEN,SAAS,CAAC;AAC1C,CAAC,CAAC;AAEJ,eAAe;EACbO,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEV,MAAM;MACZW,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAACC,KAAK,EAAE;MACxB,IAAI,CAACC,KAAK,CAAC,oBAAoB,EAAEf,MAAM,CAACc,KAAK,CAACpB,MAAM,CAACK,KAAK,CAAC,CAAC;IAC9D;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
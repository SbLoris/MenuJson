    this.showToast(message);\n    },\n    showToast(message) {\n \n    const toastElement = document.createElement(\"div\");\n    toastElement.className = \"toast\";\n    toastElement.textContent = `Ouverture de la page ${message}`;\n    \n   \n    this.$emit(\"showToastOnLinkOpen\", `Ouverture de la page ${message}`);\n\n    document.getElementById(\"toast-container\").appendChild(toastElement);\n\n    setTimeout(() => {\n      toastElement.remove();\n    }, 5000);\n  \n},\n    getComponentName(item) {\n      // Mapping entre les composants et leurs noms\n      const componentMap = {\n        homeMenuItem,\n        page1MenuItem,\n        nbArticlesField,\n        page4MenuItem,\n        page5MenuItem,\n        nbArticlesLabel,\n      };\n      return componentMap[item.component] || null;\n    },\n  },\n};\n</script>\n<style lang=\"css\" src=\"@/styles/tailwind.css\">\n\n\n/* Ajoutez des styles pour le toast si n√©cessaire */\n\n</style>\n"],"mappings":";;EACOA,EAAE,EAAC;AAAK;gCACXC,mBAAA,CAAgC;EAA3BD,EAAE,EAAC;AAAiB;;EACpBE,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAc;;EAIpBA,KAAK,EAAC;AAAyB;;uBAPxCC,mBAAA,CAwBM,OAxBNC,UAwBM,GAvBJC,UAAgC,EAChCJ,mBAAA,CAoBM,OApBNK,UAoBM,GAnBJL,mBAAA,CAGM,OAHNM,UAGM,G,gBAFJN,mBAAA,CAAmF;+DAA/DO,IAAA,CAAAC,SAAS,GAAAC,MAAA;IAAEC,WAAW,EAAC;iDAAvBH,IAAA,CAAAC,SAAS,E,GAC7BR,mBAAA,CAAmD;IAAzCW,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEN,IAAA,CAAAO,QAAA,IAAAP,IAAA,CAAAO,QAAA,IAAAD,IAAA,CAAQ;KAAE,iBAAe,E,GAE5Cb,mBAAA,CAcM,OAdNe,UAcM,I,kBAbJb,mBAAA,CAUMc,SAAA,QAAAC,WAAA,CAVuBV,IAAA,CAAAW,YAAY,KAA5BC,IAAI,EAAEC,KAAK;yBAAxBlB,mBAAA,CAUM;MAVwCmB,GAAG,EAAED;IAAK,K,cACtDE,YAAA,CAQEC,wBAAA,CAPKhB,IAAA,CAAAiB,gBAAgB,CAACL,IAAI;MACzBM,KAAK,EAAEN,IAAI,CAACM,KAAK;MACjBC,UAAU,EAAEP,IAAI,CAACO,UAAU;MAC3BC,IAAI,EAAER,IAAI,CAACQ,IAAI;MACfC,YAAY,EAAET,IAAI,CAACU,SAAS,yBAAyBtB,IAAA,CAAAuB,mBAAmB,GAAGC,SAAS;MACpFC,oBAAkB,EAAEzB,IAAA,CAAA0B,kBAAkB;MACtCC,qBAAmB,EAAE3B,IAAA,CAAA4B;;kCAGe5B,IAAA,CAAA6B,WAAW,CAACC,MAAM,Q,cAA3DnC,mBAAA,CAA+E;;IAAtES,OAAK,EAAAC,MAAA,QAAA